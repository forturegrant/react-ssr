  "scripts": {
    "build:client": "webpack --config webpack.config.client.js",
    "build:server": "webpack --config webpack.config.server.js",
    "build": "yarn build:client && yarn build:server",
    "dev:client": "webpack-dev-server --config webpack.config.client.js",
    "start": "node ./src/server",
    "lint": "npx eslint --ext .js,.jsx,.tsx src",
    "lint:fix": "npx eslint --fix --ext .js,.tsx src",
    "generateAnalyzFile": "webpack --profile --json > dist/stats.json",
    "analyzer": "webpack-bundle-analyzer --port 8888 ./dist/stats.json"
  },
